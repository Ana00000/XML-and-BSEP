AHA;AAIA,IAAA,aAJA,2BAIA;AAAA;;AACA,WAAA,KAAA,CACA,GADA,CAEA,wCACA,KAAA,YAHA,EAIA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAJA,EAUA,IAVA,CAUA,UAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,iBAAA;;AACA,YAAA,IAAA,CAAA,IAAA,CAAA,iBAAA,IAAA,OAAA,EAAA;AACA,UAAA,KAAA,CAAA,iBAAA,GAAA,sBAAA;AACA,SAFA,MAGA,IAAA,IAAA,CAAA,IAAA,CAAA,iBAAA,IAAA,SAAA,EAAA;AACA,UAAA,KAAA,CAAA,iBAAA,GAAA,wBAAA;AACA,SAFA,MAGA,IAAA,IAAA,CAAA,IAAA,CAAA,iBAAA,IAAA,SAAA,EAAA;AACA,UAAA,KAAA,CAAA,iBAAA,GAAA,wBAAA;AACA,SAFA,MAIA;AACA,UAAA,KAAA,CAAA,iBAAA,GAAA,+CAAA;AACA;;AACA,QAAA,KAAA,CAAA,cAAA,GAAA,IAAA;AAEA,OA3BA,EA4BA,KA5BA,CA4BA,UAAA,GAAA,EAAA;AACA,YAAA,KAAA,CAAA,YAAA,IAAA,EAAA,EAAA;AACA,UAAA,KAAA,CAAA,4CAAA,CAAA;AACA;;AACA,QAAA,KAAA,CAAA,cAAA,GAAA,KAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAlCA;AAmCA;AAxCA;AAZA,CAAA","sourcesContent":["<template>\r\n  <v-container fluid class=\"container\">\r\n    <h1 class=\"text-center display-1 mt-10 mb-10\">\r\n      Check the validity of the certificate based on the serial number\r\n    </h1>\r\n    <v-row aria-rowspan=\"1\">\r\n      <v-col cols=\"4\" />\r\n      <v-col cols=\"4\">\r\n        <v-text-field\r\n          label=\"Input certificate serial number\"\r\n          v-model=\"serialNumber\"\r\n          color=\"light-blue darken-4\"\r\n        />\r\n      </v-col>\r\n    </v-row>\r\n    <v-row rows=\"2\">\r\n      <v-flex text-center align-center>\r\n        <v-btn\r\n          class=\"mb-10 mt-10\"\r\n          color=\"primary\"\r\n          elevation=\"9\"\r\n          large\r\n          v-on:click=\"checkValidity\"\r\n        >\r\n          Check validity\r\n        </v-btn>\r\n      </v-flex>\r\n    </v-row>\r\n    <v-row rows=\"3\" class=\"mt-20\">\r\n      <v-col cols=\"4\" />\r\n      <v-col cols=\"4\">\r\n        <v-text-field\r\n          v-show=\"isShowValidity\"\r\n          class=\"mt-50\"\r\n          label=\"Certificate status\"\r\n          v-model=\"certificateStatus\"\r\n          color=\"light-blue darken-4\"\r\n          readonly\r\n        />\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Check certificate validity\",\r\n  data: () => ({\r\n    certificate: null,\r\n    serialNumber: \"\",\r\n    certificateStatus: null,\r\n    isShowValidity: false,\r\n    token: null,\r\n  }),\r\n  mounted() {\r\n    this.init();\r\n  },\r\n  methods: {\r\n    init() {\r\n      this.token = localStorage.getItem(\"token\");\r\n    },\r\n    checkValidity() {\r\n      this.$http\r\n        .get(\r\n          \"https://localhost:8080/certificate/\" +\r\n            this.serialNumber,\r\n          {\r\n            headers: {\r\n              'Authorization': \"Bearer \" + this.token\r\n            }\r\n          }\r\n        )\r\n        .then((resp) => {\r\n          console.log(resp.data.certificateStatus);\r\n          if (resp.data.certificateStatus == \"VALID\") {\r\n            this.certificateStatus = \"Certificate is valid\";\r\n          }\r\n          else if (resp.data.certificateStatus == \"REVOKED\") {\r\n            this.certificateStatus = \"Certificate is revoked\";\r\n          }\r\n          else if (resp.data.certificateStatus == \"EXPIRED\"){\r\n            this.certificateStatus = \"Certificate is expired\";\r\n          }\r\n          else\r\n          {\r\n            this.certificateStatus = \"Certificate not found with this serial number\";\r\n          }\r\n          this.isShowValidity = true;\r\n          \r\n        })\r\n        .catch((err) => {\r\n          if (this.serialNumber == \"\") {\r\n            alert(\"Please enter the certificate serial number\");\r\n          }\r\n          this.isShowValidity = false;\r\n          console.log(err);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views"}]}